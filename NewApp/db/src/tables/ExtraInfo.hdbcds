namespace NewApp;
using NewApp::Flower;

context ExtraInfo{

		entity Shops {
		    @Comment: 'Id of Shop'
		    key shid : String(4);
		    @Comment: 'Flower in Shop'
		    flid : String(4);
		    @Comment: 'Name'
		    name : String(100);

    		toFlower : Association[*] to Flower on toFlower.flid = flid;
		};
};