namespace NewApp;

using NewApp::ExtraInfo.Shops;


entity Flower {
    @Comment: 'Id of Flower'
    key flid : String(4);
    @Comment: 'Description'
    name : String(100);
    @Comment: 'Update time'
    ts_update : UTCDateTime;
    @Comment: 'Create time'
    ts_create : UTCDateTime;

    toShops : Association[*] to Shops on toShops.flid = flid;
};