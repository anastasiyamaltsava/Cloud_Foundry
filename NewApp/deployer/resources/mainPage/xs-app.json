{
  "welcomeFile": "/index.html",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "csrfProtection":true,
      "authenticationType": "xsuaa",
      "scope":{
        "scope":{
          "GET":[
             "$XSAPPNAME.newapp.view"
          ],
          "POST":[
             "$XSAPPNAME.newapp.edit"
          ],
          "PUT":[
             "$XSAPPNAME.newapp.edit"
          ],
          "DELETE":[
             "$XSAPPNAME.newapp.edit"
          ],
          "default":"$XSAPPNAME.newapp.view"
        }
      }
    }
  ]
}
